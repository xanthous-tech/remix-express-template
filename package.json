{
  "name": "remix-express-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format:client": "biome format ./app/*.tsx ./app/**/*.tsx ./app/*.ts ./app/**/*.ts --write",
    "format:server": "biome format ./src/*.ts ./src/**/*.ts --write",
    "build": "tsc --noEmit && biome lint ./src ./app && npm run build:client && npm run build:server",
    "build:client": "remix vite:build",
    "build:server": "tsup",
    "dev": "cross-env NODE_ENV=development tsx --watch --watch-path=./src ./src/main.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@bull-board/api": "^5.17.1",
    "@bull-board/express": "^5.17.1",
    "@remix-run/express": "^2.9.1",
    "@remix-run/node": "^2.9.1",
    "bullmq": "^5.7.8",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "ioredis": "^5.4.1",
    "pino": "^9.1.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.0.0",
    "vite": "^5.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@remix-run/dev": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "arctic": "^1.8.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.21.2",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "isbot": "^4.1.0",
    "lodash": "^4.17.21",
    "lucia": "^3.2.0",
    "lucide-react": "^0.378.0",
    "minio": "^8.0.0",
    "oslo": "^1.2.0",
    "postgres": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.1",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "20.13.1",
    "pnpm": "9.1.1"
  }
}
